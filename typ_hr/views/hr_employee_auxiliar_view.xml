<?xml version='1.0' encoding='UTF-8'?>
<openerp>
    <data>
        <record id="view_hr_employee_auxiliar_form" model="ir.ui.view">
            <field name="name">hr.employee.auxiliar.form</field>
            <field name="model">hr.employee.auxiliar</field>
            <field name="arch" type="xml">
                <form>
                    <group>
                        <field name="name"/>
                        <field name="address"/>
                        <field name="telephone"/>
                    </group>
                </form>
            </field>
        </record>

        <record id="view_hr_employee_auxiliar_tree" model="ir.ui.view">
            <field name="name">hr.employee.auxiliar.tree</field>
            <field name="model">hr.employee.auxiliar</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="address"/>
                    <field name="telephone"/>
                </tree>
            </field>
        </record>
    </data>
</openerp>
