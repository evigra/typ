<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <!--  Main Style  -->
    <template id="assets_frontend" inherit_id="website.assets_frontend" name="Assets Frontend">
        <xpath expr="." position="inside">
            <link type="text/less" rel="stylesheet" href="/theme_typ/static/src/less/main.less"/>
        </xpath>
        <xpath expr="//link[last()]" position="after">
            <link href="/theme_typ/static/src/less/fonts.less" rel="stylesheet" type="text/less"/>
            <link href="/theme_typ/static/src/less/fonts/OpenSans.less" rel="stylesheet" type="text/less"/>
            <link href="/theme_typ/static/src/less/fonts/Oswald.less" rel="stylesheet" type="text/less"/>
            <link href="/theme_typ/static/src/css/layout.css" rel="stylesheet" type="text/css"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">          
            <script type="text/javascript" src="/theme_typ/static/src/js/menu-btns.js"></script>    
            <script type="text/javascript" src="/theme_typ/static/src/js/categories.js"></script>
            <script type="text/javascript" src="/theme_typ/static/src/js/star_rating.js"></script>
        </xpath>
    </template>
</odoo>
