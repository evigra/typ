<?xml version="1.0"?>
<openerp>
    <data noupdate="1">

        <record id="filter_01" model="ir.filters">
            <field name="name">Open Invoice Lines</field>
            <field name="model_id">account.invoice.line</field>
            <field name="domain" eval="[('invoice_id.state', '=', 'open')]"/>
            <field name="user_id" eval="False"/>
        </record>

        <record id="report_base_01" model="commision.reports.base">
            <field name="name">Report for Open Invoice Lines</field>
            <field name="filter_id" eval="ref('typ_commision.filter_01')"/>
            <field name="lines" eval="True"/>
        </record>

    </data>
</openerp>
