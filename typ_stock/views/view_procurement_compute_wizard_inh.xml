<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_procurement_compute_wizard_inherit" model="ir.ui.view">
        <field name="name">view.procurement.compute.wizard.inherit</field>
        <field name="model">stock.scheduler.compute</field>
        <field name="inherit_id"
            ref="stock.view_procurement_compute_wizard"/>
        <field name="arch" type="xml">
            <xpath expr="//form" position="inside">
                <group>
                    <field name="warehouse_id" style="width: 40%"/>
                    <field name='importance' style="width: 40%"/>
                </group>
            </xpath>
        </field>
    </record>
</odoo>
