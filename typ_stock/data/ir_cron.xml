<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">

        <record id="ir_cron_procurement_cancel" model="ir.cron">
            <field name="name">Procurement exception to cancel</field>
            <field name="interval_number">7</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model" eval="'procurement.order'"/>
            <field name="function" eval="'automatic_procurement_cancel'"/>
            <field name="args" eval="'()'"/>
            <field name="active" eval="True"/>
        </record>

        <record id="ir_cron_procurement_done" model="ir.cron">
            <field name="name">Procurement running to done</field>
            <field name="interval_number">7</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="model" eval="'procurement.order'"/>
            <field name="function" eval="'automatic_procurement_done'"/>
            <field name="args" eval="'()'"/>
            <field name="active" eval="True"/>
        </record>

    </data>
</openerp>
