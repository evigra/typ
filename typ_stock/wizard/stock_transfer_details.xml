<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="view_stock_enter_transfer_details_inherit">
        <field name="name">Enter transfer details intherit</field>
        <field name="model">stock.transfer_details</field>
        <field name="inherit_id" ref="stock.view_stock_enter_transfer_details"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='product_id']" position="before">
                <field name="supplier_code" groups="typ_stock.group_reception_picking_attributes"/>
            </xpath>
            <xpath expr="//field[@name='quantity']" position="before">
                <field name="expected_quantity" groups="typ_stock.group_reception_picking_attributes"/>
            </xpath>
        </field>
    </record>
</odoo>
