<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="stock_input_account" model="account.account">
        <field name="name">Stock Input</field>
        <field name="code">StockIn</field>
        <field name="user_type_id" ref="account.data_account_type_current_assets"/>
    </record>
    <record id="stock_ouput_account" model="account.account">
        <field name="name">Stock Ouput</field>
        <field name="code">StockOut</field>
        <field name="user_type_id" ref="account.data_account_type_current_assets"/>
    </record>
    <record id="service_landed_cost_1" model="product.product">
        <field name="name">Landed Costs Service (Freight)</field>
        <field name="cost_method">standard</field>
        <field name="valuation">manual_periodic</field>
        <field name="landed_cost_ok" eval="True"/>
        <field name="uom_id" ref="product.product_uom_unit"/>
        <field name="type">service</field>
        <field name="property_stock_account_input" ref="stock_input_account"/>
        <field name="property_stock_account_output" ref="stock_ouput_account"/>
    </record>
    <record id="service_landed_cost_2" model="product.product">
        <field name="name">Landed Costs Service (Ensurance)</field>
        <field name="cost_method">standard</field>
        <field name="valuation">manual_periodic</field>
        <field name="landed_cost_ok" eval="True"/>
        <field name="uom_id" ref="product.product_uom_unit"/>
        <field name="type">service</field>
        <field name="property_stock_account_input" ref="stock_input_account"/>
        <field name="property_stock_account_output" ref="stock_ouput_account"/>
    </record>
</odoo>
