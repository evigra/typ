<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <!-- Account -->
        <record id="account_account_action" model="ir.actions.server">
            <field name="name">Account Parent Store Compute</field>
            <field name="model_id" ref="account.model_account_account"/>
            <field name="code">
                self._parent_store_compute(cr)
            </field>
        </record>
        <!-- Stock -->
        <record id="stock_location_action" model="ir.actions.server">
            <field name="name">Stock Location Parent Store Compute</field>
            <field name="model_id" ref="stock.model_stock_location"/>
            <field name="code">
                self._parent_store_compute(cr)
            </field>
        </record>
        <record id="procurement_cancel_action" model="ir.actions.server">
            <field name="name">Procurement exception to cancel</field>
            <field name="model_id" ref="typ_stock.model_procurement_order"/>
            <field name="code">
procuement_ids = env['procurement.order'].search([('state', '=', 'exception'), ('purchase_id', '!=', False)])
procuement_ids.cancel()
            </field>
        </record>
        <!-- FacturaE -->
        <record id="facturae_fix_encoding_action" model="ir.actions.server">
            <field name="name">Fix encoding on signed XML</field>
            <field name="model_id" ref="l10n_mx_ir_attachment_facturae.model_ir_attachment_facturae_mx"/>
            <field name="code">
ids = context.get('active_ids')
self.fix_xml_encoding(cr, uid, ids, context=context)
            </field>
        </record>
    </data>
</openerp>
