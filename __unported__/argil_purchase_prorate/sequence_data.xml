<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!--
    Sequence types for prorate
    -->

        <record id="seq_type_prorate" model="ir.sequence.type">
            <field name="name">Stock Prorate</field>
            <field name="code">stock.prorate</field>
        </record>
        <record id="seq_type_prorate_loc" model="ir.sequence.type">
            <field name="name">Local Prorate</field>
            <field name="code">stock.prorate.loc</field>
        </record>
        <!--
            Sequences for prorate
        -->

        <record id="seq_prorate" model="ir.sequence">
            <field name="name">Stock Prorate</field>
            <field name="code">stock.prorate</field>
            <field name="prefix">STP/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_prorate_loc" model="ir.sequence">
            <field name="name">Local Prorate</field>
            <field name="code">stock.prorate.loc</field>
            <field name="prefix">LSTP/</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
