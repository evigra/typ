<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="pruebas_sale_order_line_consulta" model="ir.ui.view">
            <field name="name">sale.order.line.check_stock</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">
                <field name="product_id" position="after">
                    <field name="consultar" on_change="consulta_stock(product_id,consultar)" attrs="{'readonly':[('product_id','=',False)]}" />
                </field>
            </field>
        </record>
    </data>
</openerp>