<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!--
    Sequence types for pickings
    -->

        <record id="seq_type_transfer_in" model="ir.sequence.type">
            <field name="name">Stock Transfer IN</field>
            <field name="code">stock.transfer.in</field>
        </record>
        
        <record id="seq_type_transfer_out" model="ir.sequence.type">
            <field name="name">Stock Transfer OUT</field>
            <field name="code">stock.transfer.out</field>
        </record>
        
        <!--
            Sequences for pickings
        -->

        <record id="seq_transfer_in" model="ir.sequence">
            <field name="name">Stock Transfer IN</field>
            <field name="code">stock.transfer.in</field>
            <field name="prefix">ST-IN/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_transfer_out" model="ir.sequence">
            <field name="name">Stock Transfer OUT</field>
            <field name="code">stock.transfer.out</field>
            <field name="prefix">ST-OUT/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</openerp>
